{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [],
    "rules": {
        "no-constant-condition": [
            "error",
            {
                "checkLoops": false
            }
        ],
        "max-len": [
            "error",
            {
                "code": 125
            }
        ],
        "linebreak-style": 0,
        "no-console": "off",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "no-await-in-loop": "off",
        "import/extensions": "off",
        "one-var": [
            "error",
            "never"
        ]
    }
}